			"name": "nginx-proxy-manager",
			"platform": "linux",
			"ports": [
				"80:8080/tcp",
				"81:8181/tcp",
				"443:4443/tcp"
			],
			"restart_policy": "unless-stopped",
			"title": "Nginx Proxy Manager",
			"type": 1,
			"volumes": [
				{
					"bind": "/srv/dev-disk-by-label-Files/Config/Nginx-Proxy",
					"container": "/config"
